{% extends "::base.html.twig" %}

{% block overraid_body %}
<body class="page-login-promo blur-page" data-blur-image="img/content/bg-login.jpg" data-blur-amount="1">

<div class="page-box">

<header class="header header-three">
  <div class="container">
    <div class="row">
      <div class="logo-box col-sm-12 col-md-12">
        <div class="logo">
		  <a href="{{ path("frontpage_index") }}">
			<img src="{{ asset("/img/gyman_logo.png") }}" class="logo-img" alt="">
		  </a>
		</div>
      </div>
    </div><!--.row -->
  </div>
</header><!-- .header -->

<section id="main">
  <div class="container login-register">
    <div class="row">
      <div class="{% block windowClasses %}col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3{% endblock windowClasses %}">
        <div class="form-box appear-animation bounceInDown appear-animation-visible" data-appear-animation="bounceInDown">
            {%block body %}
                {% block fos_user_content %}{% endblock fos_user_content %}
            {%endblock body %}
        </div>
      </div>
    </div>
  </div><!-- .container -->
</section><!-- #main -->

</div><!-- .page-box -->

    {% include ("::javascripts.html.twig") %}
</body>
{% endblock %}
    
